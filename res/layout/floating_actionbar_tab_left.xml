<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/floating_actionbar_height">
    <TextView
        android:id="@+id/tab_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|center_vertical"
        android:gravity="right|center_vertical"
        android:paddingRight="@dimen/floating_actionbar_tab_left_paddingright"
        android:layout_marginRight="@dimen/floating_actionbar_tab_left_paddingright"
        android:textColor="@color/actionbar_tab_textcolor"
        android:textSize="@dimen/floating_actionbar_tab_textsize"
        android:text="@string/tab_photos" />
</FrameLayout>
